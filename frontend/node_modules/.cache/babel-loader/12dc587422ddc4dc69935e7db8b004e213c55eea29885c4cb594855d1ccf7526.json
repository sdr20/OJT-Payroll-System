  </v-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      employees: [\r\n        { id: 1, name: 'John Doe', salary: 50000 },\r\n        { id: 2, name: 'Jane Smith', salary: 60000 }\r\n      ],\r\n      headers: [\r\n        { text: 'Employee Name', value: 'name' },\r\n        { text: 'Actions', value: 'actions', sortable: false }\r\n      ],\r\n      payHeads: ['Bonus', 'Tax', 'Allowance'],\r\n      selectedPayHeads: [],\r\n      totalEarnings: 0,\r\n      totalDeductions: 0,\r\n      netSalary: 0,\r\n      selectedEmployee: null,\r\n      payHeadsModal: false\r\n    };\r\n  },\r\n  methods: {\r\n    openPayHeadsModal(employee) {\r\n      this.selectedEmployee = employee;\r\n      this.payHeadsModal = true;\r\n    },\r\n    savePayHeads() {\r\n      this.payHeadsModal = false;\r\n      this.calculateSalary();\r\n    },\r\n    calculateSalary() {\r\n      // Example calculations\r\n      this.totalEarnings = this.selectedEmployee.salary + (this.selectedPayHeads.includes('Bonus') ? 5000 : 0);\r\n      this.totalDeductions = this.selectedPayHeads.includes('Tax') ? 2000 : 0;\r\n      this.netSalary = this.totalEarnings - this.totalDeductions;\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"mappings":"AA+DA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,CACT;QAAEC,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,UAAU;QAAEC,MAAM,EAAE;MAAM,CAAC,EAC1C;QAAEF,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,YAAY;QAAEC,MAAM,EAAE;MAAM,EAC5C;MACDC,OAAO,EAAE,CACP;QAAEC,IAAI,EAAE,eAAe;QAAEC,KAAK,EAAE;MAAO,CAAC,EACxC;QAAED,IAAI,EAAE,SAAS;QAAEC,KAAK,EAAE,SAAS;QAAEC,QAAQ,EAAE;MAAM,EACtD;MACDC,QAAQ,EAAE,CAAC,OAAO,EAAE,KAAK,EAAE,WAAW,CAAC;MACvCC,gBAAgB,EAAE,EAAE;MACpBC,aAAa,EAAE,CAAC;MAChBC,eAAe,EAAE,CAAC;MAClBC,SAAS,EAAE,CAAC;MACZC,gBAAgB,EAAE,IAAI;MACtBC,aAAa,EAAE;IACjB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,iBAAiBA,CAACC,QAAQ,EAAE;MAC1B,IAAI,CAACJ,gBAAe,GAAII,QAAQ;MAChC,IAAI,CAACH,aAAY,GAAI,IAAI;IAC3B,CAAC;IACDI,YAAYA,CAAA,EAAG;MACb,IAAI,CAACJ,aAAY,GAAI,KAAK;MAC
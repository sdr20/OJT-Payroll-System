{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.find.js\";\nimport axios from 'axios';\nexport default {\n  name: 'AdminLayout',\n  data() {\n    return {\n      totalEmployees: 0,\n      employees: [],\n      totalPresentToday: 0,\n      totalLateToday: 0,\n      totalEligibleForSalaryToday: 0,\n      showAddEmployeeModal: false,\n      showViewEmployeeModal: false,\n      newEmployee: {\n        username: '',\n        password: '',\n        firstName: '',\n        lastName: '',\n        birthday: '',\n        hireDate: '',\n        position: '',\n        civilStatus: '',\n        contactInfo: '',\n        email: '',\n        sss: '',\n        philhealth: '',\n        hdmf: ''\n      },\n      selectedEmployee: {},\n      positions: []\n    };\n  },\n  methods: {\n    async fetchEmployees() {\n      try {\n        const response = await axios.get('/api/employees');\n        this.employees = response.data;\n        this.totalEmployees = response.data.length;\n      } catch (error) {\n        console.error('Error fetching employees:', error);\n      }\n    },\n    async fetchTotalPresentToday() {\n      // Implement API call to get the number of employees present today\n    },\n    async fetchTotalLateToday() {\n      // Implement API call to get the number of employees late today\n    },\n    async fetchTotalEligibleForSalaryToday() {\n      // Implement API call to get employees eligible for salary\n    },\n    async addEmployee() {\n      try {\n        await axios.post('/api/employees', this.newEmployee);\n        this.fetchEmployees();\n        this.showAddEmployeeModal = false;\n      } catch (error) {\n        console.error('Error adding employee:', error);\n      }\n    },\n    viewEmployee(employeeId) {\n      this.selectedEmployee = this.employees.find(emp => emp.id === employeeId);\n      this.showViewEmployeeModal = true;\n    },\n    closeViewEmployeeModal() {\n      this.showViewEmployeeModal = false;\n    },\n    async removeEmployee(employeeId) {\n      try {\n        await axios.delete(`/api/employees/${employeeId}`);\n        this.fetchEmployees();\n      } catch (error) {\n        console.error('Error deleting employee:', error);\n      }\n    }\n  },\n  mounted() {\n    this.fetchEmployees();\n    this.fetchTotalPresentToday();\n    this.fetchTotalLateToday();\n    this.fetchTotalEligibleForSalaryToday();\n  }\n};","map":{"version":3,"names":["axios","name","data","totalEmployees","employees","totalPresentToday","totalLateToday","totalEligibleForSalaryToday","showAddEmployeeModal","showViewEmployeeModal","newEmployee","username","password","firstName","lastName","birthday","hireDate","position","civilStatus","contactInfo","email","sss","philhealth","hdmf","selectedEmployee","positions","methods","fetchEmployees","response","get","length","error","console","fetchTotalPresentToday","fetchTotalLateToday","fetchTotalEligibleForSalaryToday","addEmployee","post","viewEmployee","employeeId","find","emp","id","closeViewEmployeeModal","removeEmployee","delete","mounted"],"sources":["C:\\Users\\ASUS\\Desktop\\Payroll_system\\frontend\\src\\views\\AdminDashboard.vue"],"sourcesContent":["<template>\r\n  <div class=\"min-h-screen bg-gray-50\">\r\n    <!-- Top Navigation Bar -->\r\n    <nav class=\"bg-white shadow-sm\">\r\n      <div class=\"max-w-7xl mx-auto px-4 sm:px-6 lg:px-8\">\r\n        <div class=\"flex justify-between h-16\">\r\n          <div class=\"flex items-center\">\r\n            <h1 class=\"text-xl font-bold text-gray-900\">Admin Dashboard</h1>\r\n          </div>\r\n          <div class=\"flex items-center\">\r\n            <span class=\"inline-flex items-center px-3 py-1 rounded-full text-sm font-medium bg-indigo-100 text-indigo-800\">\r\n              Admin\r\n            </span>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </nav>\r\n\r\n    <div class=\"max-w-7xl mx-auto px-4 sm:px-6 lg:px-8 py-8\">\r\n      <!-- Stats Overview -->\r\n      <div class=\"grid grid-cols-1 gap-5 sm:grid-cols-2 lg:grid-cols-4\">\r\n        <!-- Total Employees Card -->\r\n        <div class=\"bg-white overflow-hidden shadow rounded-lg\">\r\n          <div class=\"p-5\">\r\n            <div class=\"flex items-center\">\r\n              <div class=\"flex-shrink-0\">\r\n                <div class=\"rounded-md bg-indigo-500 p-3\">\r\n                  <svg class=\"h-6 w-6 text-white\" fill=\"none\" stroke=\"currentColor\" viewBox=\"0 0 24 24\">\r\n                    <path strokeLinecap=\"round\" strokeLinejoin=\"round\" strokeWidth=\"2\" d=\"M17 20h5v-2a3 3 0 00-5.356-1.857M17 20H7m10 0v-2c0-.656-.126-1.283-.356-1.857M7 20H2v-2a3 3 0 015.356-1.857M7 20v-2c0-.656.126-1.283.356-1.857m0 0a5.002 5.002 0 019.288 0M15 7a3 3 0 11-6 0 3 3 0 016 0zm6 3a2 2 0 11-4 0 2 2 0 014 0zM7 10a2 2 0 11-4 0 2 2 0 014 0z\" />\r\n                  </svg>\r\n                </div>\r\n              </div>\r\n              <div class=\"ml-5 w-0 flex-1\">\r\n                <dl>\r\n                  <dt class=\"text-sm font-medium text-gray-500 truncate\">Total Employees</dt>\r\n                  <dd class=\"text-2xl font-semibold text-gray-900\">{{ totalEmployees }}</dd>\r\n                </dl>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <div class=\"bg-gray-50 px-5 py-3\">\r\n            <div class=\"text-sm\">\r\n              <a href=\"#\" @click=\"fetchEmployees\" class=\"font-medium text-indigo-600 hover:text-indigo-900\">View all</a>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- Present Today Card -->\r\n        <div class=\"bg-white overflow-hidden shadow rounded-lg\">\r\n          <div class=\"p-5\">\r\n            <div class=\"flex items-center\">\r\n              <div class=\"flex-shrink-0\">\r\n                <div class=\"rounded-md bg-green-500 p-3\">\r\n                  <svg class=\"h-6 w-6 text-white\" fill=\"none\" stroke=\"currentColor\" viewBox=\"0 0 24 24\">\r\n                    <path strokeLinecap=\"round\" strokeLinejoin=\"round\" strokeWidth=\"2\" d=\"M9 12l2 2 4-4m6 2a9 9 0 11-18 0 9 9 0 0118 0z\" />\r\n                  </svg>\r\n                </div>\r\n              </div>\r\n              <div class=\"ml-5 w-0 flex-1\">\r\n                <dl>\r\n                  <dt class=\"text-sm font-medium text-gray-500 truncate\">Present Today</dt>\r\n                  <dd class=\"text-2xl font-semibold text-gray-900\">{{ totalPresentToday }}</dd>\r\n                </dl>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <div class=\"bg-gray-50 px-5 py-3\">\r\n            <div class=\"text-sm\">\r\n              <a href=\"#\" @click=\"fetchTotalPresentToday\" class=\"font-medium text-green-600 hover:text-green-900\">View details</a>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- Late Today Card -->\r\n        <div class=\"bg-white overflow-hidden shadow rounded-lg\">\r\n          <div class=\"p-5\">\r\n            <div class=\"flex items-center\">\r\n              <div class=\"flex-shrink-0\">\r\n                <div class=\"rounded-md bg-yellow-500 p-3\">\r\n                  <svg class=\"h-6 w-6 text-white\" fill=\"none\" stroke=\"currentColor\" viewBox=\"0 0 24 24\">\r\n                    <path strokeLinecap=\"round\" strokeLinejoin=\"round\" strokeWidth=\"2\" d=\"M12 8v4l3 3m6-3a9 9 0 11-18 0 9 9 0 0118 0z\" />\r\n                  </svg>\r\n                </div>\r\n              </div>\r\n              <div class=\"ml-5 w-0 flex-1\">\r\n                <dl>\r\n                  <dt class=\"text-sm font-medium text-gray-500 truncate\">Late Today</dt>\r\n                  <dd class=\"text-2xl font-semibold text-gray-900\">{{ totalLateToday }}</dd>\r\n                </dl>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <div class=\"bg-gray-50 px-5 py-3\">\r\n            <div class=\"text-sm\">\r\n              <a href=\"#\" @click=\"fetchTotalLateToday\" class=\"font-medium text-yellow-600 hover:text-yellow-900\">View details</a>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- Eligible for Salary Card -->\r\n        <div class=\"bg-white overflow-hidden shadow rounded-lg\">\r\n          <div class=\"p-5\">\r\n            <div class=\"flex items-center\">\r\n              <div class=\"flex-shrink-0\">\r\n                <div class=\"rounded-md bg-blue-500 p-3\">\r\n                  <svg class=\"h-6 w-6 text-white\" fill=\"none\" stroke=\"currentColor\" viewBox=\"0 0 24 24\">\r\n                    <path strokeLinecap=\"round\" strokeLinejoin=\"round\" strokeWidth=\"2\" d=\"M12 8c-1.657 0-3 .895-3 2s1.343 2 3 2 3 .895 3 2-1.343 2-3 2m0-8c1.11 0 2.08.402 2.599 1M12 8V7m0 1v8m0 0v1m0-1c-1.11 0-2.08-.402-2.599-1M21 12a9 9 0 11-18 0 9 9 0 0118 0z\" />\r\n                  </svg>\r\n                </div>\r\n              </div>\r\n              <div class=\"ml-5 w-0 flex-1\">\r\n                <dl>\r\n                  <dt class=\"text-sm font-medium text-gray-500 truncate\">Eligible for Salary</dt>\r\n                  <dd class=\"text-2xl font-semibold text-gray-900\">{{ totalEligibleForSalaryToday }}</dd>\r\n                </dl>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <div class=\"bg-gray-50 px-5 py-3\">\r\n            <div class=\"text-sm\">\r\n              <a href=\"#\" @click=\"fetchTotalEligibleForSalaryToday\" class=\"font-medium text-blue-600 hover:text-blue-900\">Process payroll</a>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- Employee Table Section -->\r\n      <div class=\"mt-8 bg-white shadow overflow-hidden sm:rounded-lg\">\r\n        <div class=\"px-4 py-5 sm:px-6 flex justify-between items-center\">\r\n          <h2 class=\"text-lg leading-6 font-medium text-gray-900\">Employees Information</h2>\r\n          <button @click=\"showAddEmployeeModal = true\" class=\"inline-flex items-center px-4 py-2 border border-transparent text-sm font-medium rounded-md text-white bg-indigo-600 hover:bg-indigo-700\">\r\n            Add Employee\r\n          </button>\r\n        </div>\r\n        <div class=\"overflow-x-auto\">\r\n          <table class=\"min-w-full divide-y divide-gray-200\">\r\n            <thead class=\"bg-gray-50\">\r\n              <tr>\r\n                <th class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\">Employee ID</th>\r\n                <th class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\">Name</th>\r\n                <th class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\">Position</th>\r\n                <th class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\">Contact Info</th>\r\n                <th class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\">Email</th>\r\n                <th class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase tracking-wider\">Actions</th>\r\n              </tr>\r\n            </thead>\r\n            <tbody class=\"bg-white divide-y divide-gray-200\">\r\n              <tr v-for=\"employee in employees\" :key=\"employee.id\" class=\"hover:bg-gray-50\">\r\n                <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-900\">{{ employee.id }}</td>\r\n                <td class=\"px-6 py-4 whitespace-nowrap\">\r\n                  <div class=\"flex items-center\">\r\n                    <div class=\"h-10 w-10 flex-shrink-0\">\r\n                      <img class=\"h-10 w-10 rounded-full\" :src=\"`https://ui-avatars.com/api/?name=${employee.firstName}+${employee.lastName}`\" alt=\"\" />\r\n                    </div>\r\n                    <div class=\"ml-4\">\r\n                      <div class=\"text-sm font-medium text-gray-900\">{{ employee.firstName }} {{ employee.lastName }}</div>\r\n                    </div>\r\n                  </div>\r\n                </td>\r\n                <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-500\">{{ employee.position }}</td>\r\n                <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-500\">{{ employee.contactInfo }}</td>\r\n                <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-500\">{{ employee.email }}</td>\r\n                <td class=\"px-6 py-4 whitespace-nowrap text-right text-sm font-medium\">\r\n                  <button @click=\"viewEmployee(employee.id)\" class=\"text-indigo-600 hover:text-indigo-900 mr-3\">View</button>\r\n                  <button @click=\"removeEmployee(employee.id)\" class=\"text-red-600 hover:text-red-900\">Delete</button>\r\n                </td>\r\n              </tr>\r\n            </tbody>\r\n          </table>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Add Employee Modal -->\r\n    <div v-if=\"showAddEmployeeModal\" class=\"fixed inset-0 flex items-center justify-center bg-black bg-opacity-50\">\r\n      <div class=\"bg-white p-8 rounded shadow-md w-full max-w-3xl square-modal\">\r\n        <h2 class=\"text-2xl font-bold mb-4\">Add Employee</h2>\r\n        <form @submit.prevent=\"addEmployee\">\r\n          <div class=\"grid grid-cols-3 gap-4\">\r\n            <div class=\"mb-4\">\r\n              <label for=\"username\" class=\"block text-gray-700\">Username (for login)</label>\r\n              <input v-model=\"newEmployee.username\" type=\"text\" id=\"username\" class=\"w-full p-2 border rounded mt-1\" required>\r\n            </div>\r\n            <div class=\"mb-4\">\r\n              <label for=\"password\" class=\"block text-gray-700\">Password (for login)</label>\r\n              <input v-model=\"newEmployee.password\" type=\"password\" id=\"password\" class=\"w-full p-2 border rounded mt-1\" required>\r\n            </div>\r\n            <div class=\"mb-4\">\r\n              <label for=\"firstName\" class=\"block text-gray-700\">First Name</label>\r\n              <input v-model=\"newEmployee.firstName\" type=\"text\" id=\"firstName\" class=\"w-full p-2 border rounded mt-1\" required>\r\n            </div>\r\n            <div class=\"mb-4\">\r\n              <label for=\"lastName\" class=\"block text-gray-700\">Last Name</label>\r\n              <input v-model=\"newEmployee.lastName\" type=\"text\" id=\"lastName\" class=\"w-full p-2 border rounded mt-1\" required>\r\n            </div>\r\n            <div class=\"mb-4\">\r\n              <label for=\"birthday\" class=\"block text-gray-700\">Birthday</label>\r\n              <input v-model=\"newEmployee.birthday\" type=\"date\" id=\"birthday\" class=\"w-full p-2 border rounded mt-1\" required>\r\n            </div>\r\n            <div class=\"mb-4\">\r\n              <label for=\"hireDate\" class=\"block text-gray-700\">Hire Date</label>\r\n              <input v-model=\"newEmployee.hireDate\" type=\"date\" id=\"hireDate\" class=\"w-full p-2 border rounded mt-1\" required>\r\n            </div>\r\n            <div class=\"mb-4\">\r\n              <label for=\"position\" class=\"block text-gray-700\">Position</label>\r\n              <select v-model=\"newEmployee.position\" id=\"position\" class=\"w-full p-2 border rounded mt-1\" required>\r\n                <option v-for=\"position in positions\" :key=\"position.id\" :value=\"position.name\">{{ position.name }}</option>\r\n              </select>\r\n            </div>\r\n            <div class=\"mb-4\">\r\n              <label for=\"civilStatus\" class=\"block text-gray-700\">Civil Status</label>\r\n              <select v-model=\"newEmployee.civilStatus\" id=\"civilStatus\" class=\"w-full p-2 border rounded mt-1\" required>\r\n                <option value=\"Single\">Single</option>\r\n                <option value=\"Married\">Married</option>\r\n              </select>\r\n            </div>\r\n            <div class=\"mb-4\">\r\n              <label for=\"contactInfo\" class=\"block text-gray-700\">Contact Info (CP Number)</label>\r\n              <input v-model=\"newEmployee.contactInfo\" type=\"text\" id=\"contactInfo\" class=\"w-full p-2 border rounded mt-1\" required>\r\n            </div>\r\n            <div class=\"mb-4\">\r\n              <label for=\"email\" class=\"block text-gray-700\">Email</label>\r\n              <input v-model=\"newEmployee.email\" type=\"email\" id=\"email\" class=\"w-full p-2 border rounded mt-1\" required>\r\n            </div>\r\n            <div class=\"mb-4\">\r\n              <label for=\"sss\" class=\"block text-gray-700\">SSS</label>\r\n              <input v-model=\"newEmployee.sss\" type=\"text\" id=\"sss\" class=\"w-full p-2 border rounded mt-1\">\r\n            </div>\r\n            <div class=\"mb-4\">\r\n              <label for=\"philhealth\" class=\"block text-gray-700\">Philhealth</label>\r\n              <input v-model=\"newEmployee.philhealth\" type=\"text\" id=\"philhealth\" class=\"w-full p-2 border rounded mt-1\">\r\n            </div>\r\n            <div class=\"mb-4\">\r\n              <label for=\"hdmf\" class=\"block text-gray-700\">HDMF</label>\r\n              <input v-model=\"newEmployee.hdmf\" type=\"text\" id=\"hdmf\" class=\"w-full p-2 border rounded mt-1\">\r\n            </div>\r\n          </div>\r\n          <div class=\"flex justify-end\">\r\n            <button type=\"button\" @click=\"showAddEmployeeModal = false\" class=\"bg-gray-500 text-white font-bold py-2 px-4 rounded hover:bg-gray-700 mr-2\">Cancel</button>\r\n            <button type=\"submit\" class=\"bg-blue-500 text-white font-bold py-2 px-4 rounded hover:bg-blue-700\">Submit</button>\r\n          </div>\r\n        </form>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- View Employee Modal -->\r\n    <div v-if=\"showViewEmployeeModal\" class=\"fixed inset-0 flex items-center justify-center bg-black bg-opacity-50\">\r\n      <div class=\"bg-white p-8 rounded shadow-md w-full max-w-3xl\">\r\n        <h2 class=\"text-2xl font-bold mb-4\">Employee Details</h2>\r\n        <div>\r\n          <p><strong>Employee ID:</strong> {{ selectedEmployee.id }}</p>\r\n          <p><strong>Name:</strong> {{ selectedEmployee.firstName }} {{ selectedEmployee.lastName }}</p>\r\n          <p><strong>Position:</strong> {{ selectedEmployee.position }}</p>\r\n          <p><strong>Contact Info:</strong> {{ selectedEmployee.contactInfo }}</p>\r\n          <p><strong>Email:</strong> {{ selectedEmployee.email }}</p>\r\n          <p><strong>SSS:</strong> {{ selectedEmployee.sss }}</p>\r\n          <p><strong>Philhealth:</strong> {{ selectedEmployee.philhealth }}</p>\r\n          <p><strong>HDMF:</strong> {{ selectedEmployee.hdmf }}</p>\r\n          <p><strong>Birthday:</strong> {{ selectedEmployee.birthday }}</p>\r\n          <p><strong>Hire Date:</strong> {{ selectedEmployee.hireDate }}</p>\r\n          <p><strong>Civil Status:</strong> {{ selectedEmployee.civilStatus }}</p>\r\n        </div>\r\n        <div class=\"flex justify-end mt-4\">\r\n          <button @click=\"closeViewEmployeeModal\" class=\"bg-red-500 text-white font-bold py-2 px-4 rounded hover:bg-red-700\">Close</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'AdminLayout',\r\n  data() {\r\n    return {\r\n      totalEmployees: 0,\r\n      employees: [],\r\n      totalPresentToday: 0,\r\n      totalLateToday: 0,\r\n      totalEligibleForSalaryToday: 0,\r\n      showAddEmployeeModal: false,\r\n      showViewEmployeeModal: false,\r\n      newEmployee: {\r\n        username: '',\r\n        password: '',\r\n        firstName: '',\r\n        lastName: '',\r\n        birthday: '',\r\n        hireDate: '',\r\n        position: '',\r\n        civilStatus: '',\r\n        contactInfo: '',\r\n        email: '',\r\n        sss: '',\r\n        philhealth: '',\r\n        hdmf: ''\r\n      },\r\n      selectedEmployee: {},\r\n      positions: []\r\n    };\r\n  },\r\n  methods: {\r\n    async fetchEmployees() {\r\n      try {\r\n        const response = await axios.get('/api/employees');\r\n        this.employees = response.data;\r\n        this.totalEmployees = response.data.length;\r\n      } catch (error) {\r\n        console.error('Error fetching employees:', error);\r\n      }\r\n    },\r\n    async fetchTotalPresentToday() {\r\n      // Implement API call to get the number of employees present today\r\n    },\r\n    async fetchTotalLateToday() {\r\n      // Implement API call to get the number of employees late today\r\n    },\r\n    async fetchTotalEligibleForSalaryToday() {\r\n      // Implement API call to get employees eligible for salary\r\n    },\r\n    async addEmployee() {\r\n      try {\r\n        await axios.post('/api/employees', this.newEmployee);\r\n        this.fetchEmployees();\r\n        this.showAddEmployeeModal = false;\r\n      } catch (error) {\r\n        console.error('Error adding employee:', error);\r\n      }\r\n    },\r\n    viewEmployee(employeeId) {\r\n      this.selectedEmployee = this.employees.find(emp => emp.id === employeeId);\r\n      this.showViewEmployeeModal = true;\r\n    },\r\n    closeViewEmployeeModal() {\r\n      this.showViewEmployeeModal = false;\r\n    },\r\n    async removeEmployee(employeeId) {\r\n      try {\r\n        await axios.delete(`/api/employees/${employeeId}`);\r\n        this.fetchEmployees();\r\n      } catch (error) {\r\n        console.error('Error deleting employee:', error);\r\n      }\r\n    }\r\n  },\r\n  mounted() {\r\n    this.fetchEmployees();\r\n    this.fetchTotalPresentToday();\r\n    this.fetchTotalLateToday();\r\n    this.fetchTotalEligibleForSalaryToday();\r\n  }\r\n};\r\n</script>"],"mappings":";;AA+QA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,cAAc,EAAE,CAAC;MACjBC,SAAS,EAAE,EAAE;MACbC,iBAAiB,EAAE,CAAC;MACpBC,cAAc,EA
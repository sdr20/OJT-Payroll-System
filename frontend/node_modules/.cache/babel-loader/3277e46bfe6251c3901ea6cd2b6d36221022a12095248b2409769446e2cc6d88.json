                 <button @click=\"editEmployee(employee)\" \r\n                          class=\"text-yellow-600 hover:text-yellow-800 transition duration-200\">\r\n                    <span class=\"material-icons-outlined\">edit</span>\r\n                  </button>\r\n                    \r\n                  <!-- Remove Icon -->\r\n                  <button @click=\"removeEmployee(employee.id)\" \r\n                          class=\"text-red-600 hover:text-red-800 transition duration-200\">\r\n                    <span class=\"material-icons-outlined\">delete</span>\r\n                  </button>\r\n                </td>\r\n              </tr>\r\n            </tbody>\r\n          </table>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Side Container (Moved to the right) -->\r\n    <div class=\"w-1/4 bg-white rounded-xl shadow-sm p-6 ml-4\">\r\n      <h2 class=\"text-s font-bold text-gray-800 mb-4\">Employee Actions</h2>\r\n      <div v-if=\"selectedSideEmployee\" class=\"space-y-4\">\r\n        <div>\r\n          <p class=\"text-sm font-medium text-gray-700\">Name</p>\r\n          <p class=\"text-lg text-gray-900\">\r\n            {{ selectedSideEmployee?.firstName }} {{ selectedSideEmployee?.lastName }}\r\n          </p>\r\n        </div>\r\n        <div>\r\n          <p class=\"text-sm font-medium text-gray-700\">Position</p>\r\n          <p class=\"text-lg text-gray-900\">{{ selectedSideEmployee?.position }}</p>\r\n        </div>\r\n        <div class=\"flex flex-col space-y-2\">\r\n          <button @click=\"viewEmployeeDetails(selectedSideEmployee)\" \r\n                  class=\"bg-blue-600 text-white font-medium py-2 px-4 rounded-lg hover:bg-blue-700 transition duration-200\">\r\n            View Info\r\n          </button>\r\n          <button @click=\"approveEmployee(selectedSideEmployee)\" \r\n                  class=\"bg-green-600 text-white font-medium py-2 px-4 rounded-lg hover:bg-green-700 transition duration-200\">\r\n            Approved\r\n          </button>\r\n          <button @click=\"rejectEmployee(selectedSideEmployee)\" \r\n                  class=\"bg-red-600 text-white font-medium py-2 px-4 rounded-lg hover:bg-red-700 transition duration-200\">\r\n            Reject\r\n          </button>\r\n        </div>\r\n      </div>\r\n      <div v-else class=\"text-gray-500\">\r\n        Select an employee to view actions.\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      employees: [\r\n        { id: 1, firstName: 'John', lastName: 'Doe', position: 'Manager', salary: 3000, email: 'john.doe@example.com', \r\n          contactInfo: '123-456-7890', deductions: { sss: 200, philhealth: 150, pagibig: 100 }, earnings: { travelExpenses: 50, otherEarnings: 20 } },\r\n        // Add more employees\r\n      ],\r\n      selectedSideEmployee: null, // Ensure this is defined properly\r\n    };\r\n  },\r\n  methods: {\r\n    removeEmployee(id) {\r\n      this.employees = this.employees.filter(emp => emp.id !== id);\r\n    },\r\n    editEmployee(employee) {\r\n      console.log(\"Editing employee:\", employee);\r\n    },\r\n    viewEmployeeDetails(employee) {\r\n      console.log(\"Viewing employee details:\", employee);\r\n    },\r\n    selectSideEmployee(employee) {\r\n      this.selectedSideEmployee = employee;\r\n    },\r\n    approveEmployee(employee) {\r\n      console.log('Approved:', employee);\r\n    },\r\n    rejectEmployee(employee) {\r\n      console.log('Rejected:', employee);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Add your styles here */\r\n</style>\r\n"],"mappings":";;AA4FA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,CACT;QAAEC,EAAE,EAAE,CAAC;QAAEC,SAAS,EAAE,MAAM;QAAEC,QAAQ,EAAE,KAAK;QAAEC,QAAQ,EAAE,SAAS;QAAEC,MAAM,EAAE,IAAI;QAAEC,KAAK,EAAE,sBAAsB;QAC3GC,WAAW,EAAE,cAAc;QAAEC,UAAU,EAAE;UAAEC,GAAG,EAAE,GAAG;UAAEC,UAAU,EAAE,GAAG;UAAEC,OAAO,EAAE;QAAI,CAAC;QAAEC,QAAQ,EAAE;UAAEC,cAA
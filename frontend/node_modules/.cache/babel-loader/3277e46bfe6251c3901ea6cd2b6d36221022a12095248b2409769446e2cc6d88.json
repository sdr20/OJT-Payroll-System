{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nexport default {\n  data() {\n    return {\n      employees: [{\n        id: 1,\n        firstName: 'John',\n        lastName: 'Doe',\n        position: 'Manager',\n        salary: 3000,\n        email: 'john.doe@example.com',\n        contactInfo: '123-456-7890',\n        deductions: {\n          sss: 200,\n          philhealth: 150,\n          pagibig: 100\n        },\n        earnings: {\n          travelExpenses: 50,\n          otherEarnings: 20\n        }\n      }\n      // Add more employees\n      ],\n      selectedSideEmployee: null // Ensure this is defined properly\n    };\n  },\n  methods: {\n    removeEmployee(id) {\n      this.employees = this.employees.filter(emp => emp.id !== id);\n    },\n    editEmployee(employee) {\n      console.log(\"Editing employee:\", employee);\n    },\n    viewEmployeeDetails(employee) {\n      console.log(\"Viewing employee details:\", employee);\n    },\n    selectSideEmployee(employee) {\n      this.selectedSideEmployee = employee;\n    },\n    approveEmployee(employee) {\n      console.log('Approved:', employee);\n    },\n    rejectEmployee(employee) {\n      console.log('Rejected:', employee);\n    }\n  }\n};","map":{"version":3,"names":["data","employees","id","firstName","lastName","position","salary","email","contactInfo","deductions","sss","philhealth","pagibig","earnings","travelExpenses","otherEarnings","selectedSideEmployee","methods","removeEmployee","filter","emp","editEmployee","employee","console","log","viewEmployeeDetails","selectSideEmployee","approveEmployee","rejectEmployee"],"sources":["C:\\Users\\ASUS\\Desktop\\Payroll_system\\frontend\\src\\views\\ManageEmployees.vue"],"sourcesContent":["<template>\r\n  <div class=\"min-h-screen bg-gray-50 p-1 flex\">\r\n    <!-- Main Content -->\r\n    <div class=\"flex-1\">\r\n      <!-- Employee List -->\r\n      <div class=\"bg-white round
\n            philhealth: '1234-5678-9012',\r\n            hdmf: 'HD123456',\r\n            salary: 60000\r\n          },\r\n          {\r\n            id: 'EMP002',\r\n            firstName: 'Jane',\r\n            lastName: 'Smith',\r\n            birthday: '1990-08-30',\r\n            hireDate: '2016-07-01',\r\n            position: 'Project Manager',\r\n            civilStatus: 'Married',\r\n            contactInfo: '987-654-3210',\r\n            email: 'jane.smith@example.com',\r\n            sss: '987-65-4321',\r\n            philhealth: '4321-8765-1234',\r\n            hdmf: 'HD987654',\r\n            salary: 80000\r\n          }\r\n        ]\r\n      };\r\n    },\r\n    methods: {\r\n      editEmployee(employee) {\r\n        this.selectedEmployee = { ...employee };\r\n        this.showEditModal = true;\r\n      },\r\n      updateEmployee() {\r\n        const idx = this.employees.findIndex(emp => emp.id === this.selectedEmployee.id);\r\n        if (idx !== -1) {\r\n          this.employees[idx] = { ...this.selectedEmployee };\r\n        }\r\n        this.showEditModal = false;\r\n      },\r\n      removeEmployee(id) {\r\n        this.employees = this.employees.filter(employee => employee.id !== id);\r\n      },\r\n      addEmployee() {\r\n        if (this.newEmployee.firstName && this.newEmployee.lastName && this.newEmployee.position && this.newEmployee.salary && this.newEmployee.email) {\r\n          this.employees.push({ ...this.newEmployee });\r\n          this.newEmployee = { firstName: '', lastName: '', birthday: '', hireDate: '', position: '', civilStatus: '', contactInfo: '', email: '', sss: '', philhealth: '', hdmf: '', salary: '' };\r\n          this.showAddModal = false;\r\n        }\r\n      }\r\n    }\r\n  };\r\n  </script>\r\n  "],"mappings":";;;AA2LE,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE,KAAK;MACnBC,aAAa,EAAE,KAAK;MACpBC,gBAAgB,EAAE,IAAI;MACtBC,WAAW,EAAE;QACXC,SAAS,EAAE,EAAE;QACbC,QAAQ,EAAE,EAAE;QACZC,QAAQ,EAAE,EAAE;QACZC,QAAQ,EAAE
{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.find.js\";\nexport default {\n  data() {\n    return {\n      showAddModal: false,\n      searchEmployeeId: '',\n      searchResult: null,\n      employees: [\n      // Example employees data, replace with actual data fetching in real use case\n      {\n        id: 'EMP001',\n        firstName: 'John',\n        lastName: 'Doe',\n        position: 'Software Engineer',\n        salary: 60000,\n        email: 'john.doe@example.com'\n      }, {\n        id: 'EMP002',\n        firstName: 'Jane',\n        lastName: 'Smith',\n        position: 'Project Manager',\n        salary: 80000,\n        email: 'jane.smith@example.com'\n      }],\n      newEmployee: {\n        id: '',\n        firstName: '',\n        lastName: '',\n        position: '',\n        salary: '',\n        email: ''\n      }\n    };\n  },\n  methods: {\n    searchEmployee() {\n      this.searchResult = this.employees.find(emp => emp.id === this.searchEmployeeId) || null;\n    },\n    addEmployee() {\n      if (this.newEmployee.id && this.newEmployee.firstName && this.newEmployee.lastName && this.newEmployee.position && this.newEmployee.salary && this.newEmployee.email) {\n        this.employees.push({\n          ...this.newEmployee\n        });\n        this.newEmployee = {\n          id: '',\n          firstName: '',\n          lastName: '',\n          position: '',\n          salary: '',\n          email: ''\n        };\n        this.showAddModal = false;\n      }\n    },\n    removeEmployee(id) {\n      this.employees = this.employees.filter(employee => employee.id !== id);\n    }\n  }\n};","map":{"version":3,"names":["data","showAddModal","searchEmployeeId","searchResult","employees","id","firstName","lastName","position","salary","email","newEmployee","methods","searchEmployee","find","emp","addEmployee","push","removeEmployee","filter","employee"],"sources":["C:\\Users\\ASUS\\Desktop\\Payroll_system\\frontend\\src\\views\\ManageEmployees.vue"],"sourcesContent":["<template>\r\n    <div class=\"min-h-screen bg-gray-50 p-8\">\r\n      <h1 class=\"text-3xl font-bold mb-2 text-gray-800\">Manage Employees</h1>\r\n      <p class=\"text-gray-500 mb-6\">Add, remove, or search for employees here.</p>\r\n  \r\n      <!-- Add Employee Button -->\r\n      <button @click=\"showAddModal = true\" \r\n              class=\"bg-indigo-600 text-white font-medium py-2.5 px-6 rounded-lg hover:bg-indigo-700 \r\n                     transition duration-200 ease-in-out shadow-sm mb-6 flex items-center gap-2\">\r\n        <span class=\"material-icons-outlined\">person_add</span>\r\n        Add Employee\r\n      </button>\r\n  \r\n      <!-- Search Employee Form -->\r\n      <div class=\"bg-white rounded-xl shadow-sm p-6 mb-6\">\r\n        <form @submit.prevent=\"searchEmployee\">\r\n          <div class=\"flex items-center gap-3\">\r\n            <input v-model=\"searchEmployeeId\" \r\n                   type=\"text\" \r\n                   placeholder=\"Search by Employee ID\" \r\n                   class=\"flex-1 p-3 border border-gray-200 rounded-lg focus:ring-2 focus:ring-indigo-500 \r\n                          focus:border-indigo-500 outline-none transition duration-200\" \r\n                   required>\r\n            <button type=\"submit\" \r\n                    class=\"bg-indigo-600 text-white font-medium py-3 px-6 rounded-lg hover:bg-indigo-700 \r\n                           transition duration-200 flex items-center gap-2\">\r\n              <span class=\"material-icons-outlined\">search</span>\r\n              Search\r\n            </button>\r\n          </div>\r\n        </form>\r\n      </div>\r\n  \r\n      <!-- Employee Search Result -->\r\n      <div v-if=\"searchResult\" \r\n           class=\"bg-white rounded-xl shadow-sm p-6 mb-6 transform transition duration-300\">\r\n        <div class=\"flex justify-between items-start mb-4\">\r\n          <h2 class=\"text-2xl font-bold text-gray-800\">\r\n            {{ searchResult.firstName }} {{ searchResult.lastName }}\r\n          </h2>\r\n          <span class=\"bg-green-100 text-green-800 text-sm font-medium px-3 py-1 rounded-full\">\r\n            {{ searchResult.position }}\r\n          </span>\r\n        </div>\r\n        <div class=\"grid grid-cols-2 gap-4\">\r\n          <div v-for=\"(value, key) in searchResult\" :key=\"key\" \r\n               class=\"p-3 bg-gray-50 rounded-lg\">\r\n            <p class=\"text-sm text-gray-500 mb-1\">{{ key.charAt(0).toUpperCase() + key.slice(1) }}</p>\r\n            <p class=\"font-medium\">{{ value }}</p>\r\n          </div>\r\n        </div>\r\n      </div>\r\n  \r\n      <!-- Employee List -->\r\n      <div class=\"bg-white rounded-xl shadow-sm overflow-hidden\">\r\n        <div class=\"p-6\">\r\n          <h2 class=\"text-2xl font-bold text-gray-800 mb-4\">Employee List</h2>\r\n        </div>\r\n        <div class=\"overflow-x-auto\">\r\n          <table class=\"w-full\">\r\n            <thead class=\"bg-gray-50\">\r\n              <tr>\r\n                <th class=\"px-6 py-4 text-left text-sm font-medium text-gray-500\">Employee ID</th>\r\n                <th class=\"px-6 py-4 text-left text-sm font-medium text-gray-500\">Name</th>\r\n                <th class=\"px-6 py-4 text-left text-sm font-medium text-gray-500\">Position</th>\r\n                <th class=\"px-6 py-4 text-left text-sm font-medium text-gray-500\">Salary</th>\r\n                <th class=\"px-6 py-4 text-right text-sm font-medium text-gray-500\">Actions</th>\r\n              </tr>\r\n            </thead>\r\n            <tbody class=\"divide-y divide-gray-100\">\r\n              <tr v-for=\"employee in employees\" :key=\"employee.id\" \r\n                  class=\"hover:bg-gray-50 transition duration-200\">\r\n                <td class=\"px-6 py-4 text-sm text-gray-900\">{{ employee.id }}</td>\r\n                <td class=\"px-6 py-4 text-sm text-gray-900\">\r\n                  {{ employee.firstName }} {{ employee.lastName }}\r\n                </td>\r\n                <td class=\"px-6 py-4 text-sm text-gray-900\">{{ employee.position }}</td>\r\n                <td class=\"px-6 py-4 text-sm text-gray-900\">${{ employee.salary }}</td>\r\n                <td class=\"px-6 py-4 text-right\">\r\n                  <button @click=\"removeEmployee(employee.id)\" \r\n                          class=\"text-red-600 hover:text-red-800 font-medium text-sm transition duration-200\">\r\n                    Remove\r\n                  </button>\r\n                </td>\r\n              </tr>\r\n            </tbody>\r\n          </table>\r\n        </div>\r\n      </div>\r\n  \r\n      <!-- Add Employee Modal -->\r\n      <div v-if=\"showAddModal\" \r\n           class=\"fixed inset-0 flex items-center justify-center bg-black bg-opacity-50 z-50\">\r\n        <div class=\"bg-white rounded-xl shadow-xl w-full max-w-2xl m-4 transform transition-all\">\r\n          <div class=\"p-6 border-b\">\r\n            <h2 class=\"text-2xl font-bold text-gray-800\">Add Employee</h2>\r\n          </div>\r\n          <form @submit.prevent=\"addEmployee\" class=\"p-6\">\r\n            <div class=\"grid grid-cols-2 gap-6\">\r\n              <!-- Form fields remain the same but with updated styling -->\r\n              <div v-for=\"(value, key) in newEmployee\" :key=\"key\" class=\"space-y-2\">\r\n                <label :for=\"key\" class=\"block text-sm font-medium text-gray-700\">\r\n                  {{ key.charAt(0).toUpperCase() + key.slice(1) }}\r\n                </label>\r\n                <input v-model=\"newEmployee[key]\" \r\n                       :type=\"key === 'email' ? 'email' : key === 'salary' ? 'number' : 'text'\"\r\n                       :id=\"key\"\r\n                       class=\"w-full p-3 border border-gray-200 rounded-lg focus:ring-2 focus:ring-indigo-500 \r\n                              focus:border-indigo-500 outline-none transition duration-200\"\r\n                       required>\r\n              </div>\r\n            </div>\r\n            <div class=\"flex justify-end gap-3 mt-6\">\r\n              <button type=\"button\" \r\n                      @click=\"showAddModal = false\"\r\n                      class=\"px-6 py-2.5 border border-gray-300 rounded-lg text-gray-700 font-medium \r\n                             hover:bg-gray-50 transition duration-200\">\r\n                Cancel\r\n              </button>\r\n              <button type=\"submit\"\r\n                      class=\"px-6 py-2.5 bg-indigo-600 text-white font-medium rounded-lg \r\n                             hover:bg-indigo-700 transition duration-200\">\r\n                Submit\r\n              </button>\r\n            </div>\r\n          </form>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  export default {\r\n    data() {\r\n      return {\r\n        showAddModal: false,\r\n        searchEmployeeId: '',\r\n        searchResult: null,\r\n        employees: [\r\n          // Example employees data, replace with actual data fetching in real use case\r\n          {\r\n            id: 'EMP001',\r\n            firstName: 'John',\r\n            lastName: 'Doe',\r\n            position: 'Software Engineer',\r\n            salary: 60000,\r\n            email: 'john.doe@example.com'\r\n          },\r\n          {\r\n            id: 'EMP002',\r\n            firstName: 'Jane',\r\n            lastName: 'Smith',\r\n            position: 'Project Manager',\r\n            salary: 80000,\r\n            email: 'jane.smith@example.com'\r\n          }\r\n        ],\r\n        newEmployee: {\r\n          id: '',\r\n          firstName: '',\r\n          lastName: '',\r\n          position: '',\r\n          salary: '',\r\n          email: ''\r\n        }\r\n      };\r\n    },\r\n    methods: {\r\n      searchEmployee() {\r\n        this.searchResult = this.employees.find(emp => emp.id === this.searchEmployeeId) || null;\r\n      },\r\n      addEmployee() {\r\n        if (this.newEmployee.id && this.newEmployee.firstName && this.newEmployee.lastName && this.newEmployee.position && this.newEmployee.salary && this.newEmployee.email) {\r\n          this.employees.push({ ...this.newEmployee });\r\n          this.newEmployee = { id: '', firstName: '', lastName: '', position: '', salary: '', email: '' };\r\n          this.showAddModal = false;\r\n        }\r\n      },\r\n      removeEmployee(id) {\r\n        this.employees = this.employees.filter(employee => employee.id !== id);\r\n      }\r\n    }\r\n  };\r\n  </script>"],"mappings":";;;;AAoIE,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE,KAAK;MACnBC,gBAAgB,EAAE,EAAE;MACpBC,YAAY,EAAE,IAAI;MAClBC,SAAS,EAAE;MACT;MACA;QACEC,EAAE,EAAE,QAAQ;QACZC,SAAS,EAAE,MAAM;QACjBC,QAAQ,EAAE,KAAK;QACfC,QAAQ,EAAE,mBAAmB;QAC7BC,MAAM,EAAE,KAAK;QACbC,KAAK,EAAE;MACT,CAAC,EACD;QACEL,EAAE,EAAE,QAAQ;QACZC,SAAS,EAAE,MAAM;QACjBC,QAAQ,EAAE,OAAO;QACjBC,QAAQ,EAAE,iBAAiB;QAC3BC,MAAM,EAAE,KAAK;QACbC,KAAK,EAAE;MACT,EACD;MACDC,WAAW,EAAE;QACXN,EAAE,EAAE,EAAE;QACNC,SAAS,EAAE,EAAE;QACbC,QAAQ,EAAE,EAAE;QACZC,QAAQ,EAAE,EAAE;QACZC,MAAM,EAAE,EAAE;QACVC,KAAK,EAAE;MACT;IACF,CAAC;EACH,CAAC;EACDE,OAAO,EAAE;IACPC,cAAcA,CAAA,EAAG;MACf,IAAI,CAACV,YAAW,GAAI,IAAI,CAACC,SAAS,CAACU,IAAI,CAACC,GAAE,IAAKA,GAAG,CAACV,EAAC,KAAM,IAAI,CAACH,gBAAgB,KAAK,IAAI;IAC1F,CAAC;IACDc,WAAWA,CAAA,EAAG;MACZ,IAAI,IAAI,CAACL,WAAW,CAACN,EAAC,IAAK,IAAI,CAACM,WAAW,CAACL,SAAQ,IAAK,IAAI,CAACK,WAAW,CAACJ,QAAO,IAAK,IAAI,CAACI,WAAW,CAACH,QAAO,IAAK,IAAI,CAACG,WAAW,CAACF,MAAK,IAAK,IAAI,CAACE,WAAW,CAACD,KAAK,EAAE;QACpK,IAAI,CAACN,SAAS,CAACa,IAAI,CAAC;UAAE,GAAG,IAAI,CAACN;QAAY,CAAC,CAAC;QAC5C,IAAI,CAACA,WAAU,GAAI;UAAEN,EAAE,EAAE,EAAE;UAAEC,SAAS,EAAE,EAAE;UAAEC,QAAQ,EAAE,EAAE;UAAEC,QAAQ,EAAE,EAAE;UAAEC,MAAM,EAAE,EAAE;UAAEC,KAAK,EAAE;QAAG,CAAC;QAC/F,IAAI,CAACT,YAAW,GAAI,KAAK;MAC3B;IACF,CAAC;IA
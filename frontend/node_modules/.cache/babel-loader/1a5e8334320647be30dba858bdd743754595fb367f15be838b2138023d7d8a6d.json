{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  name: 'EmployeeLogin',\n  data() {\n    return {\n      username: '',\n      password: '',\n      showRegisterModal: false,\n      newEmployee: {\n        username: '',\n        password: '',\n        firstName: '',\n        lastName: '',\n        birthday: '',\n        hireDate: '',\n        position: '',\n        civilStatus: '',\n        contactInfo: '',\n        email: '',\n        sss: '',\n        philhealth: '',\n        hdmf: ''\n      },\n      positions: []\n    };\n  },\n  created() {\n    this.fetchPositions();\n  },\n  methods: {\n    login() {\n      this.$store.dispatch('login', {\n        username: this.username,\n        role: 'employee'\n      });\n      this.$router.push('/employee');\n    },\n    async register() {\n      try {\n        const response = await axios.post('/api/register', this.newEmployee);\n        if (response.status === 200) {\n          this.showRegisterModal = false;\n          this.resetNewEmployee();\n          this.$toast.success('Registration successful!');\n        }\n      } catch (error) {\n        console.error('Failed to register:', error);\n        this.$toast.error('Registration failed. Please try again.');\n      }\n    },\n    async fetchPositions() {\n      try {\n        const response = await axios.get('/api/positions');\n        this.positions = response.data;\n      } catch (error) {\n        console.error('Failed to fetch positions:', error);\n      }\n    },\n    resetNewEmployee() {\n      this.newEmployee = {\n        username: '',\n        password: '',\n        firstName: '',\n        lastName: '',\n        birthday: '',\n        hireDate: '',\n        position: '',\n        civilStatus: '',\n        contactInfo: '',\n        email: '',\n        sss: '',\n        philhealth: '',\n        hdmf: ''\n      };\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","username","passwor
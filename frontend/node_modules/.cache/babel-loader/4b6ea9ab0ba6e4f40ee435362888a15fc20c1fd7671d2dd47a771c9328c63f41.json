edDate: new Date().toISOString().substr(0, 10),\r\n            attendanceRecords: [\r\n                { id: 1, name: \"John Doe\", position: \"Developer\", date: \"2025-02-15\", status: \"Present\" },\r\n                { id: 2, name: \"Jane Smith\", position: \"Designer\", date: \"2025-02-15\", status: \"Absent\" },\r\n                { id: 1, name: \"John Doe\", position: \"Developer\", date: \"2025-02-16\", status: \"Absent\" },\r\n                { id: 2, name: \"Jane Smith\", position: \"Designer\", date: \"2025-02-16\", status: \"Present\" },\r\n            ]\r\n        };\r\n    },\r\n    computed: {\r\n        filteredAttendance() {\r\n            return this.attendanceRecords.filter(record => record.date === this.selectedDate);\r\n        }\r\n    }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n</style>\r\n"],"mappings":";;AAyCA,eAAe;EACXA,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,YAAY,EAAE,IAAIC,IAAI,CAAC,CAAC,CAACC,WAAW,CAAC,CAAC,CAACC,MAAM,CAAC,CAAC,EAAE,EAAE,CAAC;MACpDC,iBAAiB,EAAE,CACf;QAAEC,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,UAAU;QAAEC,QAAQ,EAAE,WAAW;QAAEC,IAAI,EAAE,YAAY;QAAEC,MAAM,EAAE;MAAU,CAAC,EACzF;QAAEJ,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,YAAY;QAAEC,QAAQ,EAAE,UAAU;QAAEC,IAAI,EAAE,YAAY;QAAEC,MAAM,EAAE;MAAS,CAAC,EACzF;QAAEJ,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,UAAU;QAAEC,QAAQ,EAAE,WAAW;QAAEC,IAAI,EAAE,YAAY;QAAEC,MAAM,EAAE;MAAS,CAAC,EACxF;QAAEJ,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,YAAY;QAAEC,QAAQ,EAAE,UAAU;QAAEC,IAAI,EAAE,YAAY;QAAEC,MAAM,EAAE;MAAU,CAAC;IAElG,CAAC;EACL,CAAC;EACDC,QAAQ,EAAE;IACNC,kBAAkBA,CAAA,EAAG;MACjB,OAAO,IAAI,CAACP,iBAAiB,CAACQ,MAAM,CAACC,MAAK,IAAKA,MAAM,CAACL,IAAG,KAAM,IAAI,CAACR,YAAY,CAAC;IACrF;EACJ;AACJ,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}
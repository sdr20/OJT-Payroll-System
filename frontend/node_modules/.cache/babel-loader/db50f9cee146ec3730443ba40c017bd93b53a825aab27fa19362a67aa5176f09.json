/div>\r\n            <div class=\"flex justify-end gap-3 mt-6\">\r\n              <button type=\"button\" \r\n                      @click=\"showEditModal = false\"\r\n                      class=\"px-6 py-2.5 border border-gray-300 rounded-lg text-gray-700 font-medium \r\n                             hover:bg-gray-50 transition duration-200\">\r\n                Cancel\r\n              </button>\r\n              <button type=\"submit\"\r\n                      class=\"px-6 py-2.5 bg-indigo-600 text-white font-medium rounded-lg \r\n                             hover:bg-indigo-700 transition duration-200\">\r\n                Update\r\n              </button>\r\n            </div>\r\n          </form>\r\n        </div>\r\n      </div>\r\n  \r\n      <!-- Add Employee Modal -->\r\n      <div v-if=\"showAddModal\" \r\n           class=\"fixed inset-0 flex items-center justify-center bg-black bg-opacity-50 z-50\">\r\n        <div class=\"bg-white rounded-xl shadow-xl w-full max-w-2xl m-4 transform transition-all\">\r\n          <div class=\"p-6 border-b\">\r\n            <h2 class=\"text-2xl font-bold text-gray-800\">Add Employee</h2>\r\n          </div>\r\n          <form @submit.prevent=\"addEmployee\" class=\"p-6\">\r\n            <div class=\"grid grid-cols-2 gap-6\">\r\n              <div v-for=\"(value, key) in newEmployee\" :key=\"key\" class=\"space-y-2\">\r\n                <label :for=\"key\" class=\"block text-sm font-medium text-gray-700\">\r\n                  {{ key.charAt(0).toUpperCase() + key.slice(1) }}\r\n                </label>\r\n                <input v-model=\"newEmployee[key]\" \r\n                       :type=\"key === 'email' ? 'email' : key === 'salary' ? 'number' : 'text'\"\r\n                       :id=\"key\"\r\n                       class=\"w-full p-3 border border-gray-200 rounded-lg focus:ring-2 focus:ring-indigo-500 \r\n                              focus:border-indigo-500 outline-none transition duration-200\"\r\n                    
{"ast":null,"code":"export default {\n  data() {\n    return {\n      employees: [{\n        id: 1,\n        name: 'John Doe',\n        position: 'Manager',\n        basicSalary: 50000,\n        allowances: 10000,\n        deductions: 5000\n      }, {\n        id: 2,\n        name: 'Jane Smith',\n        position: 'Developer',\n        basicSalary: 45000,\n        allowances: 8000,\n        deductions: 4000\n      }],\n      selectedEmployee: null\n    };\n  },\n  computed: {\n    totalEarnings() {\n      if (!this.selectedEmployee) return 0;\n      return this.selectedEmployee.basicSalary + this.selectedEmployee.allowances;\n    },\n    netSalary() {\n      if (!this.selectedEmployee) return 0;\n      return this.totalEarnings - this.selectedEmployee.deductions;\n    }\n  },\n  methods: {\n    selectEmployee(employee) {\n      this.selectedEmployee = employee;\n    },\n    generatePayslip() {\n      alert(`Payslip generated for ${this.selectedEmployee.name}`);\n    },\n    sendPayslip() {\n      alert(`Payslip sent to ${this.selectedEmployee.name}`);\n    }\n  }\n};","map":{"version":3,"names":["data","employees","id","name","position","basicSalary","allowances","deductions","selectedEmployee","computed","totalEarnings","netSalary","methods","selectEmployee","employee","generatePayslip","alert","sendPayslip"],"sources":["C:\\Users\\ASUS\\Desktop\\Payroll_system\\frontend\\src\\views\\ManageEmployees.vue"],"sourcesContent":["<template>\r\n    <div class=\"employee-list\">\r\n      <h2>Employee List</h2>\r\n      <ul>\r\n        <li v-for=\"employee in employees\" :key=\"employee.id\" @click=\"selectEmployee(employee)\">\r\n          {{ employee.name }}\r\n        </li>\r\n      </ul>\r\n      \r\n      <div v-if=\"selectedEmployee\" class=\"employee-details\">\r\n        <h3>Payslip for {{ selectedEmployee.name }}</h3>\r\n        <p><strong>Position:</strong> {{ selectedEmployee.position }}</p>\r\n        <p><strong>Basic Salary:</strong> {{ selectedEmployee.basicSalary }}</p>\r\n        <p><strong>Allowances:</strong> {{ selectedEmployee.allowances }}</p>\r\n        <p><strong>Deductions:</strong> {{ selectedEmployee.deductions }}</p>\r\n        <p><strong>Total Earnings:</strong> {{ totalEarnings }}</p>\r\n        <p><strong>Net Salary:</strong> {{ netSalary }}</p>\r\n        \r\n        <button @click=\"generatePayslip\">Generate Payslip</button>\r\n        <button @click=\"sendPayslip\">Send Payslip</button>\r\n      </div>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  export default {\r\n    data() {\r\n      return {\r\n        employees: [\r\n          { id: 1, name: 'John Doe', position: 'Manager', basicSalary: 50000, allowances: 10000, deductions: 5000 },\r\n          { id: 2, name: 'Jane Smith', position: 'Developer', basicSalary: 45000, allowances: 8000, deductions: 4000 },\r\n        ],\r\n        selectedEmployee: null\r\n      };\r\n    },\r\n    computed: {\r\n      totalEarnings() {\r\n        if (!this.selectedEmployee) return 0;\r\n        return this.selectedEmployee.basicSalary + this.selectedEmployee.allowances;\r\n      },\r\n      netSalary() {\r\n        if (!this.selectedEmployee) return 0;\r\n        return this.totalEarnings - this.selectedEmployee.deductions;\r\n      }\r\n    },\r\n    methods: {\r\n      selectEmployee(employee) {\r\n        this.selectedEmployee = employee;\r\n      },\r\n      generatePayslip() {\r\n        alert(`Payslip generated for ${this.selectedEmployee.name}`);\r\n      },\r\n      sendPayslip() {\r\n        alert(`Payslip sent to ${this.selectedEmployee.name}`);\r\n      }\r\n    }\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .employee-list ul {\r\n    list-style: none;\r\n    padding: 0;\r\n  }\r\n  \r\n  .employee-list li {\r\n    cursor: pointer;\r\n    padding: 5px;\r\n    border-bottom: 1px solid #ccc;\r\n  }\r\n  \r\n  .employee-list li:hover {\r\n    background-color: #f0f0f0;\r\n  }\r\n  \r\n  .employee-details {\r\n    margin-top: 20px;\r\n  }\r\n  </style>\r\n  "],"mappings":"AAyBE,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,CACT;QAAEC,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,UAAU;QAAEC,QAAQ,EAAE,SAAS;QAAEC,WAAW,EAAE,KAAK;QAAEC,UAAU,EAAE,KAAK;QAAEC,UAAU,EAAE;MAAK,CAAC,EACzG;QAAEL,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,YAAY;QAAEC,QAAQ,EAAE,WAAW;QAAEC,WAAW,EAAE,KAAK;QAAEC,UAAU,EAAE,IAAI;QAAEC,UAAU,EAAE;MAAK,CAAC,CAC7G;MACDC,gBAAgB,EAAE;IACpB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,aAAaA,CAAA,EAAG;MACd,IAAI,CAAC,IAAI,CAACF,gBAAgB,EAAE,OAAO,CAAC;MACpC,OAAO,IAAI,CAACA,gBAAgB,CAACH,WAAU,GAAI,IAAI,CAACG,gBAAgB,CAACF,UAAU;IAC7E,CAAC;IACDK,SAASA,CAAA,EAAG;MACV,IAAI,CAAC,IAAI,CAACH,gBAAgB,EAAE,OAAO,CAAC;MACpC,OAAO,IAAI,CAACE,aAAY,GAAI,IAAI,CAACF,gBAAgB,CAACD,UAAU;IAC9D;EACF,CAAC;EACDK,OAAO,EAAE;IACPC,cAAcA,CAACC,QAAQ,EAAE;MACvB,IAAI,CAACN,gBAAe,GAAIM,QAAQ;IAClC,CAAC;IACDC,eAAeA,CAAA,EAAG;MAChBC,KAAK,CAAC,yBAAyB,IAAI,CAACR,gBAAgB,CAACL,IAAI,EAAE,CAAC;IAC9D,CAAC;IACDc,WAAWA,CAAA,EAAG;MACZD,KAAK,CAAC,mBAAmB,IAAI,CAACR,gBAAgB,CAACL,IAAI,EAAE,CAAC;IACxD;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}